{
  "manifest_version": 2,
  "name": "Bloomberg Unblock",
  "description": "Unlimited article access to Bloomberg.com",
  "icons": {
    "16": "images/favicon16.png",
    "32": "images/favicon32.png",
    "48": "images/favicon48.png",
    "128": "images/favicon128.png"
  },
  "incognito": "split",
  "version": "1.2",
  "content_scripts": [{
    "matches": ["*://*.bloomberg.com/*"],
    "css": ["fix.css"],
    "run_at": "document_end"
  }]
}